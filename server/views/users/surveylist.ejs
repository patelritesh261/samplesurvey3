<% include ../partials/header.ejs %>
<% include ../partials/navbar.ejs %>
<main class="container">
   
<font color="green"><b><i><h2>Survey</h2></b></i></font>
    <a href="/multiple/add"><%= title %></a>
 
     <table class="table table-inverse">
   <% for (var i = 0; i < mcq.length; i++) {
    if(mcq[i].displayName == displayName  )
        {
        if(mcq[i].surveyName==surveyname){
    %>
  <tbody>
    <tr>
     
      <th colspan="7"><%= mcq[i].question %></th>
      <th>
          <div class="col-md-5 ">
           <a  href="/multiple/add/<%= mcq[i]._id %>" title="Delete" >
                        <button type="button" class="btn btn-success">Edit</button>
                    </a>
          </div>
          <div class="col-md-4">
          <a href="/multiple/delete/<%= mcq[i]._id %>" title="Delete" onclick="return confirm('Are you sure you want to delete this?');">
                        <button type="button" class="btn btn-danger">Delete</button>
                    </a>
          </div>
      </th>
    </tr>
    <tr>
      <td width="5%">A</td>
      <td width="20%"><%= mcq[i].option1 %></td>
      <td  width="5%">B</td>
      <td width="20%"><%= mcq[i].option2 %></td>
      <td width="5%">C</td>
      <td width="20%"><%= mcq[i].option3 %></td>
      <td  width="5%">D</td>
      <td width="20%"><%= mcq[i].option4 %></td>
    </tr>
   
    
  </tbody>
   <% }} }  %>
     </table>

   
</main>
<% include ../partials/footer.ejs %>