<% include ../partials/header.ejs %>
<% include ../partials/navbar.ejs %>
<main class="container">
   
<font color="green"><b><i><h2><%= surveyname %></h2></b></i></font>
 
     <table class="table table-inverse">
   <% for (var i = 0; i < mcq.length; i++) {
  
    %>
  <tbody>
    <tr>
     
      <th colspan="8"><%= mcq[i].question %></th>
      
    </tr>
    <tr>
      <td width="5%"><input type="radio" name="group<%= i %>" id="mcq" value="<%= mcq[i].option1 %>" ></td>
      <td width="20%"><%= mcq[i].option1 %></td>
      <td  width="5%"><input type="radio" name="group<%= i %>" id="mcq" value="<%= mcq[i].option2 %>" ></td>
      <td width="20%"><%= mcq[i].option2 %></td>
      <td width="5%"><input type="radio" name="group<%= i %>" id="mcq" value="<%= mcq[i].option3 %>" ></td>
      <td width="20%"><%= mcq[i].option3 %></td>
      <td  width="5%"><input type="radio" name="group<%= i %>" id="mcq" value="<%= mcq[i].option4 %>" ></td>
      <td width="20%"><%= mcq[i].option4 %></td>
    </tr>
   
    
  </tbody>
   <%  }  %>
     </table>
<div>
   
       
</div>
   <form action="" method="post">
                                             <input type="hidden" name="surveyName" value="<%= surveyname %>"/>
                                             <input type="hidden" name="displayName" value="<%= displayName %>"/>
                                              <input type="hidden" name="surveyType" value="multiple"/>
                                               <input type="email" name="fromadd" value="<%= fromadd %>" placeholder="Email"/>
                                           <a href=""> <button type="submit" class="btn btn-success" >Submit Survey</button></a>
                                         
                                            </form>
</main>
<% include ../partials/footer.ejs %>